{
    "version": 2,
    "name": "chat_app",
    "buildCommand": "npm start",
    "installCommand": "npm install",
    "regions": [
        "fra1"
    ],
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.ts"
        }
    ]
}