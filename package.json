{
  "name": "chat_app_socket_io",
  "version": "1.0.0",
  "description": "API SOCKET built with NodeJS, Express, MongoDB and TypeScript",
  "main": "src/server.ts",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "env-dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev": "node -r ./node_modules/.bin/ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "watch-ts": "tsc -w",
    "prod": "npm run build && npm run start",
    "eject": "nodejs-scripts eject",
    "eslint": "eslint --ext js,ts",
    "lint": "eslint './src/**/*.{js,ts}'",
    "lint:fix": "eslint './src/**/*.{js,ts}' --fix",
    "prettify": "prettier --write"
  },
  "dependencies": {
    "@types/bad-words": "3.0.1",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.13",
    "@types/jsonwebtoken": "8.5.8",
    "@types/mongoose": "5.11.97",
    "@types/morgan": "1.9.4",
    "@types/socket.io": "3.0.2",
    "ansi-regex": "6.0.1",
    "aws-sdk": "2.1328.0",
    "bad-words": "^3.0.4",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "1.4.6",
    "date-format": "^3.0.0",
    "debug": "4.3.4",
    "dotenv": "^10.0.0",
    "engine.io": "6.2.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "express": "4.18.1",
    "jsonwebtoken": "^8.5.1",
    "minimist": "1.2.6",
    "mongoose": "^6.0.8",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "sharp": "0.31.3",
    "socket.io": "^4.2.0",
    "ts-node-dev": "2.0.0",
    "typescript": "4.6.4"
  },
  "eslintConfig": {
    "extends": "nodejs"
  },
  "engines": {
    "node": ">=14 <15"
  },
  "devDependencies": {
    "@types/multer": "1.4.5",
    "@types/sharp": "0.31.1"
  }
}
