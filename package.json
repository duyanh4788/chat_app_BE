{
  "name": "chat_app_socket_io",
  "version": "1.0.0",
  "description": "API SOCKET built with NodeJS, Express, MongoDB and TypeScript",
  "main": "src/server.ts",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "env-dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev": "node -r ./node_modules/.bin/ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "watch-ts": "tsc -w",
    "prod": "npm run build && npm run start",
    "eject": "nodejs-scripts eject",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "dependencies": {
    "@aws-sdk/lib-storage": "3.293.0",
    "@types/bad-words": "3.0.1",
    "@types/bcryptjs": "2.4.2",
    "@types/cookie-parser": "1.4.3",
    "@types/express": "4.17.13",
    "@types/express-session": "1.17.6",
    "@types/jsonwebtoken": "8.5.8",
    "@types/mongoose": "5.11.97",
    "@types/morgan": "1.9.4",
    "@types/nodemailer": "6.4.7",
    "@types/passport": "1.0.12",
    "@types/passport-facebook": "2.1.11",
    "@types/passport-google-oauth20": "2.0.11",
    "@types/socket.io": "3.0.2",
    "@types/winston": "2.4.4",
    "aws-client-s3": "1.4.5",
    "aws-sdk": "2.1328.0",
    "bad-words": "^3.0.4",
    "bcryptjs": "^2.4.3",
    "connect-mongo": "5.0.0",
    "cookie-parser": "1.4.6",
    "debug": "4.3.4",
    "dotenv": "^10.0.0",
    "engine.io": "6.2.0",
    "express": "4.18.1",
    "express-session": "1.17.3",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "5.1.0",
    "mongoose": "^6.0.8",
    "morgan": "1.10.0",
    "multer": "1.4.5-lts.1",
    "nodemailer": "6.9.1",
    "passport": "0.6.0",
    "passport-facebook": "3.0.0",
    "passport-google-oauth20": "2.0.0",
    "sharp": "0.31.3",
    "socket.io": "^4.2.0",
    "ts-node-dev": "2.0.0",
    "winston": "3.8.2"
  },
  "engines": {
    "node": ">=14 <15"
  },
  "devDependencies": {
    "@types/multer": "1.4.5",
    "@types/sharp": "0.31.1",
    "@typescript-eslint/eslint-plugin": "5.54.1",
    "@typescript-eslint/parser": "5.54.1",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.7.0",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.8.4",
    "typescript": "4.9.5"
  }
}